{
    "name": "v-button repeater",
    "short_name": "v-button repeater",
    "description": "repeat button playback",
    "version": "1.0.0",
    "manifest_version": 2,
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "background": {
        "scripts": ["js/background.js"]
    },
    "browser_action": {
        "default_title": "",
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "web_accessible_resources": [
        "*.png",
        "*.eot",
        "*.woff",
        "*.woff2",
        "*.ttf",
        "*.svg"
    ],
    "permissions": [
        "storage"
    ]
}
