{
    "name": "v-button",
    "short_name": "v-button",
    "description": "Vのボタンを自動再生します",
    "version": "1.0.0",
    "manifest_version": 2,
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://www.natorisana.love/",
                "*://vampsasha.starfree.jp/",
                "*://kanae.starfree.jp/",
                "*://928button.tudura.com/",
                "*://ushiumi.ichiya-boshi.net/",
                "*://himawari.live/"
            ],
            "js": ["js/content.js"]
        }
    ],
    "browser_action": {
        "default_title": "",
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "web_accessible_resources": [
        "*.png",
        "*.eot",
        "*.woff",
        "*.woff2",
        "*.ttf",
        "*.svg"
    ],
    "permissions": [
        "storage"
    ]
}
